#include<stdio.h>
#include<string.h>
int main()
{
    int nf=0,i=0,j=0,ch;
    char mdname[10],fname[10][10],name[10];
    printf("enter the directory name:");
    scanf("%s",&mdname);
    printf("enter the numebr of files:\n");
    scanf("%d",&nf);
    do
    {
        printf("enter file name:\n");
        scanf("%s",name);
        for(i=0;i<nf;i++)
    {
        if(!strcmp(name,fname[i]))
        break;
    }
    if (i==nf)
    {
    strcpy(fname[j++],name);
    nf++;
    }
    else
    printf("file namwe already taken:\n",name);
    printf("do want you enter again:\n");
    scanf("%d",&ch);
    }
    while(ch==1);
    {
        printf("enter the directory:\n",mdname);
        printf("file names are:\n");
        for(i=0;i<j;i++)
        printf("%s",fname[i]);
        return 0;
    }
}
